# Root environment variables
# Copy this file to .env and fill in your values

# Environment
NODE_ENV=production

# Backend
BACKEND_PORT=5000

# API Keys (필수)
WEATHER_API_KEY=your_openweather_api_key_here

# Security Settings
# CORS 허용 도메인 (쉼표로 구분, 배포 후 실제 도메인으로 변경)
ALLOWED_ORIGINS=https://your-domain.vercel.app,https://www.your-domain.com

# Rate Limiting (15분당 최대 요청 수)
RATE_LIMIT_MAX=100

# Frontend
REACT_APP_API_URL=/api
REACT_APP_NAVER_MAP_CLIENT_ID=your_naver_map_client_id_here
FRONTEND_PORT=3000
